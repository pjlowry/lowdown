          <!-- Three columns of text below the carousel -->
          <div class="container">
            <div class="row">
              <%= form_for Contact.new do |f| %>
                <div class="span4">            
                  <div class="control-group">
                    <%= f.label :name %>
                    <div class="controls">
                      <%= f.text_field :name, :placeholder => "Name ..." %>
                    </div>
                  </div>
                  <div class="control-group">
                    <%= f.label :email %>
                    <div class="controls">
                      <%= f.text_field :email, :placeholder => "Email ..." %>
                    </div>
                  </div>
                </div><!-- /.span4 -->
                <div class="span7">
                  <div class="control-group">
                    <%= f.label :message %>
                    <%= f.text_area :message, rows: 10, :placeholder => "Message ..."%>
                  </div>
                </div>
                <div class="span4">
                  <%= f.submit "Submit", :class => 'btn' %>
                </div>     
              <% end %> 
            </div><!-- /.row -->
          </div>